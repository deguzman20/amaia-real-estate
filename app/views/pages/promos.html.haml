.container
  %br/
  %br/
  .row
    .col-md-8.col-lg-8
      - @promos.each do |promo|
        %h3
          = "Amaia #{promo.try(:type).try(:name)} #{promo.try(:unit).try(:specific_location)}- Promos"
        %br/
        = image_tag promo.try(:image).url(:thumb).to_s
    .col-md-3.col-lg-3
      = render 'shared/list_of_projects', available_locations: @available_locations